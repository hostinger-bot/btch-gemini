{
  "name": "btch-gemini",
  "version": "5.0.0",
  "description": "A package for communicating with the Gemini API for chat and image processing.",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.cjs.json && tsc -p tsconfig.esm.json",
    "start": "node dist/index.js",
    "test": "jest",
    "prepublishOnly": "npm run build",
    "prepare": "npm run build"
  },
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ],
  "config": {
    "baseUrl": "https://gemini-api.zone.id",
    "description": "A package for communicating with the Gemini API for chat and image processing."
  },
  "keywords": [
    "gemini",
    "chat",
    "image-processing",
    "video-processing",
    "audio-processing",
    "image-editing",
    "api",
    "typescript"
  ],
  "author": "BOTCAHX",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.8.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hostinger-bot/btch-gemini.git"
  },
  "bugs": {
    "url": "https://github.com/hostinger-bot/btch-gemini/issues"
  },
  "homepage": "https://github.com/hostinger-bot/btch-gemini#readme",
  "directories": {
    "src": "src",
    "test": "test"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "readme": "README.md",
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": ["**/test/*.ts", "**/test/**/*.ts"],
    "moduleFileExtensions": ["ts", "js"],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    }
  }
}
